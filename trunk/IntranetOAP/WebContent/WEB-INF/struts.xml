<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml" />
	<include file="jasperreports-default.xml" />
	<package name="default" extends="jasperreports-default, struts-default" namespace="/relatorio">
		<result-types>
			<result-type name="jasper" class="org.apache.struts2.views.jasperreports.JasperReportsResult" />
		</result-types>
		<action name="gerarLaudoPdf" class="br.com.oappr.infra.action.OlaMundoAction">
			<result name="success" type="jasper">
				<param name="location">/reports/TONOMETRIA.jasper</param>
				<param name="dataSource">listaLaudos</param>
				<param name="format">PDF</param>
			</result>
		</action>
	
	</package>
</struts>
